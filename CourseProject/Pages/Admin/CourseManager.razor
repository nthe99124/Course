    private CreateCourseVM createCourse { get; set; } = new CreateCourseVM();
    private Guid? courseId { get; set; }
    private Dictionary<Guid, string> listTeacher { get; set; } = new Dictionary<Guid, string>();
    private IBrowserFile selectedFile;
    private InputFile fileInput;
    protected override async Task HandleAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            listTeacher = await _accountService.GetTeacherList();
            StateHasChanged();
        }

    }

    private void OnChangeTimePeriod(TypeOfTerm typeOfTerm)
    {
        createCourse.TypeOfTerm = typeOfTerm;
    }

    private async Task CreateOrUpdateCourse()
    {
        if (createCourse != null)
        {

            if (selectedFile != null)
            {
                createCourse.ImgCourseFile = await ConvertFileToBase64(selectedFile);    
            }

            var res = await _courseService.CreateCourseMaster(createCourse);
            if (res != null && res.IsSuccess && res.Data != null)
            {
                ShowSuccessMessage("Đăng ký khóa học thành công, tiếp tục thực hiện đăng ký các bài học.");
                courseId = res.Data;
                NavigateToLession();
            }
            else
            {
                ShowErrorMessage(res.Message);
            }
        }
    }

    private async Task HandleFileSelected(InputFileChangeEventArgs e)
    {
        var files = e.GetMultipleFiles();

        selectedFile = files.FirstOrDefault();
    }

    public async Task addFile()
    {
        await InvokeVoidAsync("openFilePicker", fileInput.Element);
        StateHasChanged();
    }

    public void NavigateToLession()
    {
        if (courseId != null)
        {
            NavigateToUrl($"/admin/lession/{courseId}", true);
        }
    }
}
