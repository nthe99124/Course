@page "/admin/course"
@using CourseProject.Model.ViewModel.Course
@using CourseProject.Services
@using CourseProject.Shared.ComponentBase
@using static CourseProject.Model.Enum.DataType

@inject IAccountService _accountService;
@inject ICourseService _courseService;
@inherits AuthorBaseComponent

<link href="https://hocvienthueonline.edubit.vn/statics/css/detail_course.css" rel="stylesheet">
<link href="/css/Page/Admin/courseManager.css" rel="stylesheet">

<Toasts class="p-3" Messages="messages" Placement="toastsPlacement" AutoHide="true" Delay="6000"/>
<div id="detail_course" class="tabs container-page container-page-edit-course">
    <div class="container-fluid bg">
        <div class="container">
            <input type="hidden" id="page-redirect" value="1">
            <h4><a href="https://hocvienthueonline.edubit.vn/manage#courses" class="btn btn-link back-link-custom btn-back-link-custom"><i class="fas fa-chevron-circle-left"></i></a> Khóa học: @createCourse.CourseName</h4>
            <div class="clear-both-zone"></div>
            <Tabs>
                <Tab Title="Giới thiệu" IsActive @onclick='(() => NavigateToUrl("/admin/course"))'>
                    <Content>
                        
                    </Content>
                </Tab>
                <Tab Title="Bài học" Disabled="@(courseId == null)"  IsActive @onclick='NavigateToLession'>
                    <Content>
                        
                    </Content>
                </Tab>
                @* <Tab Title="Cài đặt">
                    <Content>
                        
                    </Content>
                </Tab> *@
            </Tabs>
        </div>
    </div>
    <div class="container-fluid bg-container container-edit-course-bg">
        <div class="container acting wrap-content-cms container-edit-course">
            <div class="tab-content tab-main">
                <div class="tab-pane active show" id="info-course">
                        <div id="page-courses">
                            <div class="container">
                                <div class="document-course-alert alert alert-danger" role="alert" style="display: none">
                                    <strong>Khóa học dạng tài liệu nhưng tính năng đã hết hạn!!</strong>
                                </div>
                                <form name="create_course" id="create_course" data-toggle="validator" method="POST">
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="row setup-content" id="step-1">
                                                        <div class="form-group col-md-12 has-feedback">
                                                            <label class="control-label">Tên khóa học <span class="text-danger">*</span></label>
                                                            <input type="text" @bind="createCourse.CourseName" class="form-control input-step" data placeholder="Nhập tên khóa học" maxlength="100">
                                                        </div>
                                                </div>

                                                <div class="row setup-content" id="step-1">
                                                    <div class="form-group col-md-6 has-feedback">
                                                        <label class="control-label"></label>
                                                        <select class="form-control" @bind="@createCourse.Teacher">
                                                            <option>Chọn giảng viên</option>
                                                            @if(listTeacher != null && listTeacher.Count >0)
                                                            {
                                                                @foreach (var item in listTeacher)
                                                                {
                                                                    <option value="@item.Key">@item.Value</option>
                                                                }
                                                            }
                                                        </select>
                                                    </div>
                                                </div>
                                                
                                                <div class="row setup-content type-course" id="step-1">
                                                        <div class="form-group col-md-12">
                                                            <label class="control-label">Video giới thiệu
                                                                <a href="#" title="Nhập đường link Youtube giới thiệu về khóa học"></a>
                                                            </label>
                                                            <input type="text" id="video_ad" @bind="createCourse.VideoDescription" name="video_ad" class="form-control input-step" placeholder="https://www.youtube.com/watch?v=m303WqGQPAM">
                                                        </div>
                                                </div>
                                                <div class="row setup-content" id="step-1">
                                                        <div class="form-group col-md-12  has-feedback">
                                                            <label class="control-label">Mô tả khóa học 
                                                            </label>
                                                            <textarea cols="5" @bind="createCourse.Description" class="form-control" id="description_courses" name="description_courses" placeholder="Nhập mô tả khóa học" maxlength="250"></textarea>
                                                        </div>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="row section">
                                                    <div class="col-md-12 mb-3">
                                                        <p class="control-label">Hình đại diện (500x300)</p>
                                                        <div class="upload-cover">
                                                            <a class="btnupload-thumbnail btnupload">
                                                            <img id="previewImage" src="" class="preview-thumbnail">
                                                            <InputFile OnChange="HandleFileSelected" id="fileInput" style="display:none" @ref="fileInput" />
                                                            <span class="text-btnupload" @onclick="addFile"><i class="fas fa-image"></i> Chọn ảnh</span></a>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="row setup-content" id="step-2">
                                            <div class="form-group col-md-12 row">
                                                <label class="control-label" style="margin-left: 5px;">Giới thiệu khóa học <span class="text-danger">*</span>
                                                </label>
                                                <textarea id="course_info" class="form-control" cols="100" name="course_info" style="margin-left: 15px;" rows="10" @bind="createCourse.Introduce">

                                                </textarea>
                                            </div>
                                        </div>
                                    <div class="row setup-content section" id="step-4">
                                        <div class="col-md-11">
                                            <div class="row">
                                                <div class="col-md-12" style="margin-bottom: 15px;">
                                                    <fieldset class="filedset-site col-md-12">
                                                        <legend class="legend-site">
                                                            <span class="title-legend">Thời hạn học</span>
                                                        </legend>
                                                        <div class="col-md-12 item-filedset mt-0">
                                                            <label class="label_radio d-flex" style="column-gap: 5px">
                                                                <input type="radio" value="unlimit" name="type_expired" checked="" @onchange="() => OnChangeTimePeriod(TypeOfTerm.Lifetime)">
                                                                <span class="my-auto">Trọn đời</span>
                                                            </label>
                                                            <label class="label_radio d-flex" style="column-gap: 5px">
                                                                <input type="radio" value="limit" name="type_expired" @onchange="() => OnChangeTimePeriod(TypeOfTerm.MonthLimit)">
                                                                <span class="my-auto">Có thời hạn</span>
                                                                <input type="number" class="form-control input-number" value="0" style="width: 80px" disabled="@(createCourse.TypeOfTerm == TypeOfTerm.Lifetime || createCourse.TypeOfTerm == TypeOfTerm.DateLimit)" name="month_expired" min="1">
                                                                <span class="my-auto">tháng</span>
                                                            </label>
                                                            <label class="label_radio d-flex" style="column-gap: 5px">
                                                                <input type="radio" value="limit-day" name="type_expired" @onchange="() => OnChangeTimePeriod(TypeOfTerm.DateLimit)">
                                                                <span class="my-auto">Có thời hạn</span>
                                                                <input type="number" class="form-control input-number" value="0" style="width: 80px" disabled="@(createCourse.TypeOfTerm == TypeOfTerm.Lifetime || createCourse.TypeOfTerm == TypeOfTerm.MonthLimit)" name="day_expired" min="1">
                                                                <span class="my-auto">ngày</span>
                                                            </label>
                                                        </div>
                                                    </fieldset>
                                                </div>
                                                <div class="col-md-12">
                                                    <fieldset class="filedset-site col-md-12">
                                                        <legend class="legend-site">
                                                            <span class="title-legend">Học phí</span>
                                                        </legend>
                                                        <div class="col-md-12 item-filedset mb-0">
                                                            <div class="row">
                                                                <div class="col-sm-12">
                                                                    <ul class="list-unstyled data-list mb-0">
                                                                        <li>
                                                                            <div class="form-group mb-0">
                                                                                <div class="inline-group">
                                                                                    <label class="label_radio">
                                                                                        <input type="radio" value="free" name="price_check" checked="" @onclick="() => { createCourse.TypeOfPurchase = TypeOfPurchase.Free; }"> <span class="label-text">Miễn phí</span>
                                                                                    </label>
                                                                                    <label class="label_radio">
                                                                                        <input type="radio" value="paid" name="price_check" @onclick="() => { createCourse.TypeOfPurchase = TypeOfPurchase.Paid; }"> <span class="label-text">Trả phí</span>
                                                                                    </label>
                                                                                    <label class="label_radio">
                                                                                        <input type="radio" value="no_retail" name="price_check" @onclick="() => { createCourse.TypeOfPurchase = TypeOfPurchase.NotForRetail; }"> <span class="label-text">Không bán lẻ (Bán combo hoặc membership)</span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                        </li>
                                                                    </ul>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-12 item-filedset mt-0">
                                                            <div class="currency-filedset-zone " id="hide-paid" style="@(createCourse.TypeOfPurchase == TypeOfPurchase.Paid ? "display: block": "display: none")" >
                                                                <div class="row">
                                                                    <div class="col-md-6">
                                                                        <h4 class="leanheader">Giá thành</h4>
                                                                        <div class="row">
                                                                            <div class="col-md-6">
                                                                                <div class="form-group">
                                                                                    <label class="control-label">Giá gốc</label>
                                                                                    <input type="text" @bind="createCourse.Price" class="form-control">
                                                                                </div>
                                                                            </div>
                                                                            <div class="col-md-6">
                                                                                <div class="form-group">
                                                                                    <label class="control-label">Giá bán sau khuyến mại</label>
                                                                                    <input type="text" @bind="createCourse.PriceAfterDiscount" class="form-control">
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </fieldset>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row setup-content section">
                                        <div class="col-md-10 col-md-offset-1 mt-10">
                                            <div id="save_button">
                                                <div class="btn btn-success" type="button" @onclick="async() => await CreateOrUpdateCourse()"><i class="fa fa-save"></i> Cập nhật
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                </div>
                <div class="tab-pane upload_files_lesson" id="lessons">
                    <div class="col-sm form-group list-unstyled add-chapter-zone">
                        <a href="javascript:;" class="btn btn-small btn-info" onclick="LHM.lessons.setting_general_lesson('64b16ab2e275cb37128b4572')"><i class="fa fa-cog"></i> Cài đặt chung</a>
                        <a href="javascript:;" class="btn btn-small btn-success btn-add-chapter" data-courseid="64b16ab2e275cb37128b4572"><i class="fa fa-plus"></i> Thêm chương</a>
